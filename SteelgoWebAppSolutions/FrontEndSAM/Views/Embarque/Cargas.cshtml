@{

    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section breadcrumb {
    <li>
        <a href="@Url.Action("landing", "Home")"><span id="lblInicio"></span></a>
    </li>
    <li>
        <a href="#"><span id="lblEmbarque"></span></a>
    </li>
    <li class="active">
        <a href=@Url.Action("Cargas", "Embarque")><span id="lblCargas"></span></a>
    </li>
}


<div id="FormEmbarqueCargas" class="form clearfix col-xs-12 col-sm-12 col-md-12 col-lg-12">
    @*Menu superior*@
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <input type="hidden" value=@ViewBag.EmbarquePlanaID id="inputHiddenEmbarquePlanaID" />
                    <div class="btn-group save-group">
                        <button id="Guardar" onclick="javascript:void(0);" type="button" class="btn btn-yellow"><span id="botonGuardar"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar"><span id="botonGuardar2"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevo"><span id="botonGuardarYNuevo"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar"></span></a>
                </div>
                <div id="CargasEmbarqueDivSuperior" class="button-section">
                    <a id="CargasEmbarqueSuperior" href=@Url.Action("Embarque", "Embarque") class="btn btn-primary"><span id="botonEmbarque"></span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones1"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones">
                            <li><a href="#" id="btnEmbarqueCerrarPlana"><span id="EmbarqueCargaBotonCerrarPlana"></span></a></li>

                        </ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
    @*Controles Superior*@
    <div class="row">
        <div class="col-sm-6 col-md-3 col-lg-3 altoControl">
            <label id="EmbarqueCargaProveedor"></label>
            <input id="inputProveedor" />
        </div>
        <div class="col-sm-6 col-md-3 col-lg-3 altoControl">
            <label id="EmbarqueCargaPLaca"></label>
            <input id="inputEmbarqueCargaPLacaPlana" />
        </div>
        <div class="col-sm-6 col-md-3 col-lg-3 altoControl">
            <label>&nbsp;</label>
            <label id="EmbarqueCargaTotalPiezas">5</label>
        </div>
        <div class="col-sm-6 col-md-3 col-lg-3 altoControl">
            <label>&nbsp;</label>
            <label id="EmbarqueCargaToneladasCargadas"> 15</label>
            <label id="LabelProyecto" style="display:none;"> </label>
        </div>
        @*<div class="col-sm-6 col-md-6 col-lg-6">
              
            </div>*@
        <div class="col-sm-12 col-md-12 col-lg-12 altoControl">
            <hr />
        </div>
        <div class="col-sm-2 col-md-2 col-lg-3">
            <div class="btn-group " role="group">
                <label id="EmbarqueCargaSelecciona"></label>
                <label id="styleReporte">
                    <input type="radio" name="EmbarqueCargaTipoSeleccion" style="width:auto;" value="Spool" /><span id="EmbarqueCargaOpcionSpool"></span>
                </label>
                <label id="styleListado">
                    <input type="radio" name="EmbarqueCargaTipoSeleccion" style="width:auto;" value="Paquete" /><span id="EmbarqueCargaOpcionPaquete"> </span>
                </label>
                <label id="styleListado">
                    <input type="radio" name="EmbarqueCargaTipoSeleccion" style="width:auto;" value="Codigo" /><span id="EmbarqueCargaOpcionCodigo"> </span>
                </label>
            </div>
        </div>
        <div class="col-sm-3 col-md-3 col-lg-3" id="divSpool">
            <label id="EmbarqueCargaSpool"></label>
            <input style="width:40%" id="InputOrdenTrabajo" class="item-select" />-<input id="InputID" placeholder="ID" style="width:40%;" />
        </div>
        <div class="col-sm-3 col-md-3 col-lg-3" id="divPaquete">
            <label id="EmbarqueCargaPaquete"></label>
            <input id="inputPaquete" />
        </div>
        <div class="col-sm-3 col-md-3 col-lg-2" id="divCodigo">
            <label id="EmbarqueCargaCodigo"></label>
            <input id="inputCodigo" />
        </div>
        <div class="col-sm-3 col-md-3 col-lg-1">
            <label id="">&nbsp;</label>
            <button type="button" id="btnAgregar" class="btn btn-blue"><span id="EmbarqueCargaAgregar"></span></button>

        </div>
    </div>
    @*GRID*@
    <div class="row addedSectionInLine">
        <div class="col-sm-8 col-md-8 col-lg-8">

        </div>
        <div class="col-sm-4 col-md-4 col-lg-4">
            <button id="ButtonCrearPaquete" onclick="javascript:void(0);" type="button" class="btn btn-yellow"><span id="EmbarqueCargaCrearPaquete"></span></button>
            <button id="ButtonAgregarPAquete" onclick="javascript:void(0);" type="button" class="btn btn-yellow"><span id="EmbarqueCargaAgregarAPaquete"></span></button>
            <br /><br />
        </div>
        <div id="ContenedorGrid" class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div id="grid" data-role="grid" class="k-grid k-widget">
            </div>
        </div>
        <input id="hdAccionFiltrado" type="hidden" />
    </div>
    @*Menu Inferior*@
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="Guardar" onclick="javascript:void(0);" type="button" class="btn btn-yellow"><span id="botonGuardar3"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar1"><span id="botonGuardar4"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevo1"><span id="botonGuardarYNuevo1"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar2"></span></a>
                </div>
                <div id="CargasEmbarqueDivInferior" class="button-section">
                    <a id="CargasEmbarqueInferior" href=@Url.Action("Embarque", "Embarque") class="btn btn-primary"><span id="botonEmbarque2"></span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones2"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones">
                            <li><a id="btnEmbarqueCerrarPlana2"><span id="EmbarqueCargaBotonCerrarPlana1"></span></a></li>
                        </ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
</div>
<div id="ventanaPopup" style="display: none; z-index:inherit;">
    <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10">
        <input id="inputPopupCuadrante" />
    </div>
    <br /><br /><br /><br />
    <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10">
        <button id="btnActualizarCuadrantePopUp" type="button" class="btn btn-blue"><span id="EmbarqueCargaActualizarCuadrantePopup"></span></button>
        <button id="btnCancelarPopUp" type="button" class="btn btn-black"><span id="EmbarqueCargaCancelarPopup"></span></button>
        <input type="hidden" id="inputPopupPaqueteID" />
        <input type="hidden" id="inputPopupSpoolID" />
    </div>
</div>

<div id="ventanaPaquetePopup" style="display: none; z-index:inherit;">
    <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10">
        <label id="EmbarqueCargaPopupPaquete"></label>
        <input id="inputPopupPaquete" />
    </div>
    <br /><br /><br /><br /> 
    <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10">
        <button id="btnActualizarPaquetePopUp" type="button" class="btn btn-blue"><span id="EmbarqueCargaActualizarPaquetePopup"></span></button>
        <button id="btnCancelarPaquetePopUp" type="button" class="btn btn-black"><span id="EmbarqueCargaCancelarPaquetePopup"></span></button>
    </div>
</div>

<script type="text/javascript" src="~/Scripts/AppComponents/Embarque/Cargas/Ajax.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Embarque/Cargas/Handlers.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Embarque/Cargas/Render.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Embarque/Cargas/Captura.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Embarque/Cargas/Seguridad.js"></script>


<script>
    @section JavascriptDocumentReadyFunctions {
    $authorizationModel["CargaEmbarque"] = $EmbarqueCargaModel;
    }
</script>

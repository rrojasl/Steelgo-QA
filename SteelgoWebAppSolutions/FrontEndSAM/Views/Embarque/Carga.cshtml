    <div id="windowPackageEmpty" style="display:none">
        <div class="container-fluid" style="margin:0px">
            <div class="form-group">
                @*<center>*@
                <span id="EmbarqueEmpaquetadoMsjDescargaEliminaPaquete"></span>
                <input type="hidden" id="detallePaquete"/>
                @*</center>*@
            </div>
            <div class="form-group row" id="divDescargarPaquete">
                <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1"></div>

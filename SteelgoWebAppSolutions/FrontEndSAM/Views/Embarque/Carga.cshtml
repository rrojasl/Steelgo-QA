    <div id="divNuevoProveedor" style="display: none; z-index:inherit;">
        <div class="col-sm-12 col-md-12 col-lg-12" style="width: 100%; padding-right:0px; ">
            <div class="row">
                <div class="col-sm-10 col-md-10 col-lg-10" id="divNombreNuevoProveedor">
                    <label id="">Proveedor</label>
                    </br>
                    <input id="inputNombreNuevoProveedor" class="general-input form-control"/>
                </div>
            </div>
            </br>
            </br>
            <div class="row">
                <a class=" btn btn-primary col-sm-4 col-md-4 col-lg-4" href="#" id="GuardarNuevoProveedor">
                    <span id="">Guardar</span>
                </a>
                <div class="col-sm-1 col-md-1 col-lg-1"></div>
                <a class=" btn btn-primary col-sm-4 col-md-4 col-lg-4" href="#" id="CancelarNuevoProveedor">
                    <span id="">Cancelar</span>
                </a>
            </div>
        </div>
    </div>
    @*NUEVA PLANA POPUP*@
    <div id="divNuevoPlana" style="display: none; z-index:inherit;">
        <div class="col-sm-12 col-md-12 col-lg-12" style="width: 100%; padding-right:0px; ">
            <div class="row">
                <div class="form-group">
                    <div class="col-sm-10 col-md-10 col-lg-10" id="divNombreNuevaPlana">
                        <label id="">Plana</label>
                        </br>
                        <input id="inputNombreNuevaPlana" class="general-input form-control"/>
                    </div>
                </div>
            </div>
            </br>
            </br>
            <div class="row">
                <a class=" btn btn-primary col-sm-4 col-md-4 col-lg-4" href="#" id="GuardarNuevaPlana">
                    <span id="">Guardar</span>
                </a>
                <div class="col-sm-1 col-md-1 col-lg-1"></div>
                <a class=" btn btn-primary col-sm-4 col-md-4 col-lg-4" href="#" id="CancelarNuevaPlana">
                    <span id="">Cancelar</span>
                </a>
            </div>
        </div>
    </div>
    @*Descarga de Spool*@
    <div id="ventanaPopup" style="display: none; z-index:inherit;">
        <div class="container-fluid">
            <div class="form-group">
                <span id="EmbarqueCargaMsjDescargaSpool"></span>
            </div>
            <div class="form-group row">
                <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1"></div>
                <div class="col-xs-4 col-sm-3 col-md-3 col-lg-3">
                    <input id="inputZonaPopup" placeholder="Zona" />
                </div>
                <div class="col-xs-4 col-sm-3 col-md-3 col-lg-3">
                    <input id="inputCuadrantePopup" placeholder="Cuadrante" />
                </div>
                <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1"></div>
            </div>
            <div class="row">
                <center>
                    <a class="btn btn-primary confirm_yes" href="#/" id="btnDescargar">
                        <span id="PinturaCargaDescargar"></span>
                    </a>
                    <a class="btn btn-primary confirm_yes" href="#/" id="btnCerrarPopup">
                        <span id="PinturaCargaCerrarPopup"></span>
                    </a>
                </center>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="GuardarPie" onclick="javascript:void(0);"  type="button" class="btn btn-yellow  accionGuardar"><span id="botonGuardar3"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar1" class="accionGuardar"><span id="BtnGuardarCerrarEmabarqueInf"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevo1"><span id="botonGuardarYNuevo1"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar1"></span></a>
                </div>
                <div id="EntregaresultadosDivINF" class="button-section">
                    <a id="EntregaresultadosINF" href=@Url.Action("EmbarqueCarro", "Embarque") class="btn btn-primary"><span id="">Preparación Embarque</span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones2"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones">
                            <li>@*<a href="#" id="btnEmbarqueCerrarPlana" class="btnCerrarPlana"><span id="EmbarqueCargaBotonCerrarPlana">Cerrar plana</span></a>*@</li>
                        </ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
</div>


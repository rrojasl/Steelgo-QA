@{
    
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section breadcrumb {
    <li>
        <a href="@Url.Action("landing", "Home")"><span id="lblInicio"></span></a>
    </li>
    <li>
        <a href="#"><span id="lblEmbarque">Embarque</span></a>
    </li>
    <li class="active">
        <a href=@Url.Action("Cargas", "Embarque")><span id="lblCargas">Carga plana</span></a>
    </li>
}


<div id="FormEmbarqueCargas" class="form clearfix col-xs-12 col-sm-12 col-md-12 col-lg-12">
    @*Menu superior*@
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <input type="hidden" value=@ViewBag.EmbarquePlanaID id="inputHiddenEmbarquePlanaID" />
                    <div class="btn-group save-group">
                        <button id="Guardar" onclick="javascript:void(0);" type="button" class="btn btn-yellow accionGuardar"><span id="botonGuardar"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar" class="accionGuardar"><span id="BtnGuardarCerrarEmabarqueSup"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevo"><span id="botonGuardarYNuevo"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar"></span></a>
                </div>
                <div id="CargasEmbarqueDivSuperior" class="button-section">
                    <a id="CargasEmbarqueSuperior" href=@Url.Action("EmbarqueCarro", "Embarque") class="btn btn-primary"><span id="botonEmbarque">Preparación Embarque</span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones1"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones">
                            <li>@*<a href="#" id="btnEmbarqueCerrarPlana" class="btnCerrarPlana"><span id="EmbarqueCargaBotonCerrarPlana">Cerrar plana</span></a>*@</li>

                        </ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
    @*Controles Superior*@
    <div class="row addedSectionInLineWithoutColor">
        <div class="col-xs-12 col-sm-4 col-md-2 col-lg-2 altoControl" id="divProyecto">
            <label id="lblProyecto"></label>
            <input id="inputProyecto" />
        </div>
        <div class="col-xs-12 col-sm-4 col-md-2 col-lg-2 altoControl" id="divProveedor">
            <label id="EmbarqueCargaProveedor"></label>
            <input id="inputProveedor" />
        </div>
        <div class="col-xs-12 col-sm-4 col-md-2 col-lg-2 altoControl" id="divPlana">
            <label id="EmbarqueCargaPLaca"></label>
            <input id="inputEmbarqueCargaPLacaPlana" />
        </div>
        <div class="col-xs-12 col-sm-2 col-md-1 col-lg-1 FieldSetView">
            <br />
            <button class="btn btn-blue" id="btnMostrar"><span id="botonMostrar"></span></button>
        </div>

        <div class="col-xs-12 col-sm-4 col-md-1 col-lg-1 altoControl">
            <label id="lblCerrar"></label>
            <input id="inputCerrar" type="checkbox">
        </div>
        <div class="col-xs-12 col-sm-4 col-md-1 col-lg-1 altoControl">
            <label id="EmbarqueCargaTotalPiezas">Total piezas</label>
            <label id="lblEmbarqueCargaTotalPiezas"></label>
        </div>
        <div class="col-xs-12 col-sm-4 col-md-2 col-lg-2 altoControl">
            <label id="EmbarqueCargaToneladasCargadas">Toneladas cargadas</label>
            <label id="lblEmbarqueCargaToneladasCargadas"></label>            
        </div>
        @*<div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 altoControl">
                <hr />
        </div>*@
        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">
            <div class="btn-group " role="group">
                <label id="EmbarqueCargaSelecciona"></label>
                <label id="styleReporte">
                    <input type="radio" name="EmbarqueCargaTipoSeleccion" class="radioEmbarqueCargaTipoSeleccion" style="width:auto;" value="Spool"  /><span id="EmbarqueCargaOpcionSpool">Spool</span>
                </label>
                <label id="styleListado">
                    <input type="radio" name="EmbarqueCargaTipoSeleccion" class="radioEmbarqueCargaTipoSeleccion" style="width:auto;" value="Paquete" /><span id="EmbarqueCargaOpcionPaquete"> Paquete</span>
                </label>
                <label id="styleListado">
                    <input type="radio" name="EmbarqueCargaTipoSeleccion" class="radioEmbarqueCargaTipoSeleccion" style="width:auto;" value="Codigo" /><span id="EmbarqueCargaOpcionCodigo">Codigo </span>
                </label>
            </div>
        </div>
        <div class="col-xs-4 col-sm-3 col-md-2 col-lg-2" id="divSpool">
            <label id="lblSpoolID"></label>
            <input style="width:40%" id="InputOrdenTrabajo" class="item-select general-input" />-<input id="InputID" placeholder="ID" style="width:40%;" />
        </div>
        <div class="col-xs-4 col-sm-3 col-md-2 col-lg-2" id="divPaquete" style="display:none;">
            <label id="EmbarqueCargaPaquete">Paquete</label>
            <input id="inputPaquete" />
        </div>
        <div class="col-xs-4 col-sm-3 col-md-2 col-lg-2" id="divCodigo" style="display:none;">
            <label id="EmbarqueCargaCodigo">Código</label>
            <input id="inputCodigo" class="item-select general-input" />
        </div>
        <div class="col-xs-12 col-sm-3 col-md-3 col-lg-1">
            <label id="">&nbsp;</label>
            <button type="button" id="btnAgregar" class="btn btn-blue"><span id="EmbarqueCargaAgregar"></span></button>
        </div>
    </div>

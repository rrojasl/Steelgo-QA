@{
    ViewBag.Title = "Carga";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section breadcrumb {
    <li>
        <a href="@Url.Action("landing", "Home")"><span id="lblInicio"></span></a>
    </li>
    <li>
        <a href="#"><span id="lblEmbarque">Embarque</span></a>
    </li>
    <li class="active">
        <a href=@Url.Action("Cargas", "Embarque")><span id="lblCargas">Carga plana</span></a>
    </li>
}


<div id="FormEmbarqueCargas" class="form clearfix col-xs-12 col-sm-12 col-md-12 col-lg-12">
    @*Menu superior*@
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <input type="hidden" value=@ViewBag.EmbarquePlanaID id="inputHiddenEmbarquePlanaID" />
                    <div class="btn-group save-group">
                        <button id="Guardar" onclick="javascript:void(0);" type="button" class="btn btn-yellow accionGuardar"><span id="botonGuardar"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar" class="accionGuardar"><span id="BtnGuardarCerrarEmabarqueSup"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevo"><span id="BtnGuardarCerrarNuevoEmbarqueSup"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar"></span></a>
                </div>
                <div id="CargasEmbarqueDivSuperior" class="button-section">
                    <a id="CargasEmbarqueSuperior" href=@Url.Action("EmbarqueCarro", "Embarque") class="btn btn-primary"><span id="botonEmbarque">Preparación Embarque</span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones1"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones">
                            <li><a href="#" id="btnEmbarqueCerrarPlana" class="btnCerrarPlana"><span id="EmbarqueCargaBotonCerrarPlana">Cerrar plana</span></a></li>

                        </ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
    @*Controles Superior*@
    <div class="row">
        <div class="col-xs-12 col-sm-4 col-md-2 col-lg-2 altoControl" id="divProveedor">
            <label id="EmbarqueCargaProveedor">Proyecto</label>
            <input id="inputProyecto" />
        </div>
        <div class="col-xs-12 col-sm-4 col-md-2 col-lg-2 altoControl" id="divProveedor">
            <label id="EmbarqueCargaProveedor">Proveedor</label>
            <input id="inputProveedor" />
        </div>
        <div class="col-xs-12 col-sm-4 col-md-2 col-lg-2 altoControl" id="divPlana">
            <label id="EmbarqueCargaPLaca">Plana</label>
            <input id="inputEmbarqueCargaPLacaPlana" />
        </div>
        <div class="col-xs-12 col-sm-2 col-md-1 col-lg-1 FieldSetView">
            <br />
            <button class="btn btn-blue" id="btnMostrar"><span>Mostrar</span></button>
        </div>

        <div class="col-xs-12 col-sm-4 col-md-1 col-lg-1 altoControl">
            <label>Cerrar</label>
            <input   id="lblEstatus" type="checkbox">
        </div>
        <div class="col-xs-12 col-sm-4 col-md-1 col-lg-1 altoControl">
            <label id="EmbarqueCargaTotalPiezas">Total piezas</label>
            <label id="lblEmbarqueCargaTotalPiezas"></label>
        </div>
        <div class="col-xs-12 col-sm-4 col-md-2 col-lg-2 altoControl">
            <label id="EmbarqueCargaToneladasCargadas">Toneladas cargadas</label>
            <label id="lblEmbarqueCargaToneladasCargadas"></label>
            <label id="LabelProyecto" style="display:none;">Proyecto</label>
        </div>
        
        

        @*<div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 altoControl">
                <hr />
            </div>*@

        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">
            <div class="btn-group " role="group">
                <label id="EmbarqueCargaSelecciona">Tipo carga</label>
                <label id="styleReporte">
                    <input type="radio" name="EmbarqueCargaTipoSeleccion" class="radioEmbarqueCargaTipoSeleccion" style="width:auto;" value="Spool" checked="checked" /><span id="EmbarqueCargaOpcionSpool">Spool</span>
                </label>
                <label id="styleListado">
                    <input type="radio" name="EmbarqueCargaTipoSeleccion" class="radioEmbarqueCargaTipoSeleccion" style="width:auto;" value="Paquete" /><span id="EmbarqueCargaOpcionPaquete"> Paquete</span>
                </label>
                <label id="styleListado">
                    <input type="radio" name="EmbarqueCargaTipoSeleccion" class="radioEmbarqueCargaTipoSeleccion" style="width:auto;" value="Codigo" /><span id="EmbarqueCargaOpcionCodigo">Codigo </span>
                </label>
            </div>
        </div>
        <div class="col-xs-4 col-sm-3 col-md-2 col-lg-2" id="divSpool">
            <label id="lblSpoolID"></label>
            <input style="width:40%" id="InputOrdenTrabajo" class="item-select general-input" />-<input id="InputID" placeholder="ID" style="width:40%;" />
        </div>
        <div class="col-xs-4 col-sm-3 col-md-2 col-lg-2" id="divPaquete" style="display:none;">
            <label id="EmbarqueCargaPaquete">Paquete</label>
            <input id="inputPaquete" />
        </div>
        <div class="col-xs-4 col-sm-3 col-md-2 col-lg-2" id="divCodigo" style="display:none;">
            <label id="EmbarqueCargaCodigo">Código</label>
            <input id="inputCodigo" class="item-select general-input" />
        </div>
        <div class="col-xs-12 col-sm-3 col-md-3 col-lg-1">
            <label id="">&nbsp;</label>
            <button type="button" id="btnAgregar" class="btn btn-blue"><span id="EmbarqueCargaAgregar">Agregar</span></button>

        </div>
    </div>
    @*GRID*@
    <div class="row addedSectionInLine">
        <div class="col-sm-8 col-md-8 col-lg-8">

        </div>

        <div id="ContenedorGrid" class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div id="grid" data-role="grid" class="k-grid k-widget">
            </div>
        </div>
        <input id="hdAccionFiltrado" type="hidden" />
    </div>
    @*NUEVO PROVEDOR POPUP*@
    <div id="windowPopUp" style="display: none; z-index:inherit;">
        <div class="col-sm-11 col-md-11 col-lg-11">
            <div class="row">
                <div class="form-group">
                    <div id="ContenedorWindowPopUp">
                        <div class="col-sm-3 col-md-3 col-lg-2" id="divNombreNuevoProveedor">
                            <label id="lblNombreNuevoProveedor"></label>
                            <input id="inputNombreNuevoProveedor" />
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-2" id="divNuevoProveedorDesc">
                            <label id="lblNuevoProveedorDesc"></label>
                            <input id="inputNuevoProveedorDesc" />
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-2" id="divNuevoProveedorDir">
                            <label id="lblNuevoProveedorDir"></label>
                            <input id="inputNuevoProveedorDir" />
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-2" id="divNuevoProveedorTel">
                            <label id="lblNuevoProveedorTel"></label>
                            <input id="inputNuevoProveedorTel" />
                        </div>
                    </div>
                </div>
                <div class="row">
                    <a class=" btn btn-primary col-sm-4 col-md-4 col-lg-4" href="#" id="GuardarNuevoProveedor">
                        <span id="textoGuardar"></span>
                    </a>
                    <div class="col-sm-1 col-md-1 col-lg-1"></div>
                    <a class=" btn btn-primary col-sm-4 col-md-4 col-lg-4" href="#" id="CancelarNuevoProveedor">
                        <span id="textoCancelar"></span>
                    </a>
                </div>
            </div>
        </div>
        @*Menu Inferior*@
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="formNav clearfix">
                    <div class="button-section editButtonSection">
                        <div class="btn-group save-group">
                            <button id="Guardar1" onclick="javascript:void(0);" type="button" class="btn btn-yellow accionGuardar"><span id="botonGuardar3"></span></button>
                            <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <span class="caretWhite"></span>
                            </button>
                            <ul class="dropdown-menu">
                                <li><a href="#" id="btnGuardar1" class="accionGuardar"><span id="botonGuardar4"></span></a></li>
                                <li><a href="#" id="btnGuardarYNuevo1"><span id="botonGuardarYNuevo1"></span></a></li>
                            </ul>
                        </div>
                        <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar2"></span></a>
                    </div>
                    <div id="CargasEmbarqueDivInferior" class="button-section">
                        <a id="CargasEmbarqueInferior" href=@Url.Action("Embarque", "Embarque") class="btn btn-primary"><span id="botonEmbarque2">Empaquetado</span></a>
                    </div>
                    <div class="button-section actionButtonSection">
                        <div class="dropdown action-group">
                            <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                                <span id="botonAcciones2"></span>
                                <span class="caretBlue"></span>
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="Acciones">
                                <li><a id="btnEmbarqueCerrarPlana2" class="btnCerrarPlana"><span id="EmbarqueCargaBotonCerrarPlana1"></span></a></li>
                            </ul>
                        </div>
                    </div>
                    <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
                </div>
            </div>
        </div>
    </div>
    <div id="ventanaPopup" style="display: none; z-index:inherit;">
        <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10">
            <input id="inputPopupCuadrante" />
        </div>
        <br /><br /><br /><br />
        <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10">
            <button id="btnActualizarCuadrantePopUp" type="button" class="btn btn-blue"><span id="EmbarqueCargaActualizarCuadrantePopup"></span></button>
            <button id="btnCancelarPopUp" type="button" class="btn btn-black"><span id="EmbarqueCargaCancelarPopup"></span></button>
            <input type="hidden" id="inputPopupPaqueteID" />
            <input type="hidden" id="inputPopupSpoolID" />
        </div>
    </div>

    <div id="ventanaPaquetePopup" style="display: none; z-index:inherit;">
        <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10">
            <label id="EmbarqueCargaPopupPaquete"></label>
            <input id="inputPopupPaquete" />
        </div>
        <br /><br /><br /><br />
        <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10">
            <button id="btnActualizarPaquetePopUp" type="button" class="btn btn-blue"><span id="EmbarqueCargaActualizarPaquetePopup"></span></button>
            <button id="btnCancelarPaquetePopUp" type="button" class="btn btn-black"><span id="EmbarqueCargaCancelarPaquetePopup"></span></button>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="GuardarPie" onclick="javascript:void(0);" type="button" class="btn btn-yellow"><span id="botonGuardar3"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar1"><span id="BtnGuardarCerrarEmabarqueInf"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevo1"><span id="BtnGuardarCerrarNuevoEmbarqueInf"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar1"></span></a>
                </div>
                <div id="EntregaresultadosDivINF" class="button-section">
                    <a id="EntregaresultadosINF"  href=@Url.Action("EmbarqueCarro", "Embarque") class="btn btn-primary"><span id="">Preparación Embarque</span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones2"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones">
                            <li><a href="#" id="btnEmbarqueCerrarPlana" class="btnCerrarPlana"><span id="EmbarqueCargaBotonCerrarPlana">Cerrar plana</span></a></li>
                        </ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript" src="~/Scripts/AppComponents/Embarque/Cargas/Ajax.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Embarque/Cargas/Handlers.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Embarque/Cargas/Render.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Embarque/Cargas/Captura.js"></script>


@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section breadcrumb {
    <li>
        <a href="@Url.Action("landing", "Home")"><span id="lblInicio"></span></a>
    </li>
    <li>
        <a href="#"><span id="lblEmbarque"></span></a>
    </li>
    <li class="active">
        <a href="@Url.Action("RevisionEmbarque", "Embarque")"><span id="EmbarqueRevisionBreadcrumb"></span></a>
    </li>
}
<div id="FormRevisionEmbarque" class="form clearfix col-xs-12 col-sm-12 col-md-12 col-lg-12">
    @*Menu superior*@
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="Guardar" onclick="javascript:void(0);" type="button" class="btn btn-yellow accionGuardar"><span id="botonGuardar"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#/" id="btnGuardar" class="accionGuardar"><span id="botonGuardar2"></span></a></li>
                            <li><a href="#/" id="btnGuardarYNuevo" class="accionGuardarNuevo"><span id="botonGuardarYNuevo"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar"></span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#/" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones1"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones"></ul>
                    </div>
                </div>
                <a id="Imprimir" href="#/" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
    <div id="FieldSetView">
        <div class="row">
            <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2" id="divProyecto">
                <label id="lblProyecto"></label>
                <input id="Proyecto" />
            </div>
            <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2" id="divEmbarque">
                <label id="lblEmbarque1"></label>
                <input id="Embarque" />
            </div>
            <div class="col-xs-12 col-sm-6 col-md-2 col-lg-2 altoControl">
                <label id="lblMostrar"></label>
                <input type="radio" name="Muestra" class="radio-responsive" style="width:auto;" value="SinCaptura" /><span id="lblSinCaptura"></span>
                <input type="radio" name="Muestra" class="radio-responsive" style="width:auto;" value="Todos" /><span id="lblTodos"> </span>
            </div>
            <div class="col-xs-12 col-sm-1 col-md-1 col-lg-1">
                <label id="">&nbsp;</label>
                <button type="button" id="btnBuscar" class="btn btn-blue"><span id="botonMostrar"></span></button>
            </div>
            <div class="col-xs-12 col-sm-1 col-md-1 col-lg-1 altoControl" style="text-align:center;">
                <label id="lblCerrar"></label>
                <input id="InputCerrar" type="checkbox">
            </div>
            <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">
                <label id="lblDestino"></label>
                <label id="EmbarqueDestino"></label>
            </div>
        </div>
        <br />
        <div class="row clearfix">
            <div class="addedSection col-sm-12 col-md-12 col-lg-12">
                <div class="row">
                    <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">
                        <div class="btn-group " role="group">
                            <label id="lblTipoCarga"></label>
                            <label id="styleReporte">
                                <input type="radio" name="RevisionEmbarqueTipoSeleccion" style="width:auto;" value="Spool" /><span id="lblSpool"></span>
                            </label>
                            <label id="styleListado">
                                <input type="radio" name="RevisionEmbarqueTipoSeleccion" style="width:auto;" value="Paquete" /><span id="lblPaquete"></span>
                            </label>
                            <label id="styleListado">
                                <input type="radio" name="RevisionEmbarqueTipoSeleccion" style="width:auto;" value="Codigo" /><span id="lblCodigo"></span>
                            </label>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2" id="divSpool">
                        <label id="lblSpoolID"></label>
                        <input style="width:40%" id="InputOrdenTrabajo" class="item-select general-input" />-<input id="InputID" placeholder="ID" style="width:40%;" />
                    </div>
                    <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2" id="divPaquete" style="display:none;">
                        <label id="RevisionEmbarquePaquete">Paquete</label>
                        <input id="inputPaquete" />
                    </div>
                    <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2" id="divCodigo" style="display:none;">
                        <label id="EmbarqueCargaCodigo">Código</label>
                        <input id="inputCodigo" class="item-select general-input" />
                    </div>
                    <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">
                        <label id="">&nbsp;</label>
                        <button type="button" id="btnAgregar" class="btn btn-blue"><span id="RevisionEmbarqueAgregar">Agregar</span></button>
                    </div>                   
                </div>
            </div>
        </div>
        <div class="row addedSectionInLine">
            <div class="form-group col-sm-12 col-md-12 col-lg-12">
                <div id="ContenedorGrid" >
                    <div id="grid" data-role="grid" class="k-grid k-widget">
                    </div>
                </div>
            </div>
        </div>
        <input id="hdAccionFiltrado" type="hidden" />
    </div>
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="Guardar1" onclick="javascript:void(0);" type="button" class="btn btn-yellow accionGuardar"><span id="botonGuardar3"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#/" id="btnGuardar1" class="accionGuardar"><span id="botonGuardar4"></span></a></li>
                            <li><a href="#/" id="btnGuardarYNuevo1" class="accionGuardarNuevo"><span id="botonGuardarYNuevo1"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar1"></span></a>
                </div>                
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#/" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones2"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones"></ul>
                    </div>
                </div>
                <a id="Imprimir" href="#/" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
</div>
<div id="windowDownloadSpool" style="display:none">
    <div class="container-fluid" style="margin:0px">
        <div class="form-group">
            @*<center>*@
            <span id="spanDescargaSpool"></span>
            <input type="hidden" id="inputTipoDescarga"/>
            @*</center>*@
        </div>
        <div class="row">
            <center>
                <a class="btn btn-primary confirm_yes" href="#/" id="btnDescargarSpool">
                    <span id="botonSi"></span>
                </a>
                <a class="btn btn-primary confirm_yes" href="#/" id="btnCancelarDescarga">
                    <span id="botonNo"></span>
                </a>
            </center>
        </div>
    </div>
</div>

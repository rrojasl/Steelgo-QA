

@{
    ViewBag.Title = "Encintar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section breadcrumb {
    <li>
        <a href="@Url.Action("landing", "Home")"><span id="lblInicio">Inicio</span></a>
    </li>
    <li>
        <a href="#"><span id="lblEmbarque">Embarque</span></a>
    </li>
    <li class="active">
        <a href="@Url.Action("Encintado", "Embarque")"><span id="lblEncintado">Encintado</span></a>
    </li>
}
<div id="FormEmbarqueMarcado" class="form clearfix col-xs-12 col-sm-12 col-md-12 col-lg-12">
    @*Menu superior*@
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="Guardar" onclick="javascript:void(0);" type="button" class="btn btn-yellow"><span id="botonGuardar"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar"><span id="botonGuardar2"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevo"><span id="botonGuardarYNuevo"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar"></span></a>
                </div>
                <div id="EntregaresultadosDivSup" class="button-section">
                    <a id="EntregaresultadosSup" class="btn btn-primary"><span id="">Carga de planas</span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones1"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones">

                            <li><a href="#" id="ImprimirTravel"><span id="EmbarqueMarcadoImprimirTravelMasivo">Imprimir Travel Masivo</span></a></li>
                        </ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
    @*Controles Superior*@
    <div class="row" id="FieldSetView">
        <div class="col-xs-12 col-sm-6 col-md-2 col-lg-2 altoControl" id="AreaDiv">
            <label id=""><span id="EmbarqueMarcadoArea">Proyecto</span></label>
            <input id="Proyecto" />
        </div>
        <div class="col-xs-12 col-sm-6 col-md-2 col-lg-2 altoControl" id="AreaDiv">
            <label id=""><span id="EmbarqueMarcadoArea">Area</span></label>
            <input id="Area" />
        </div>
        <div class="col-xs-12 col-sm-6 col-md-2 col-lg-2 altoControl" id="CuadranteDiv">
            <label id=""><span id="EmbarqueMarcadoCuadrante">Cuadrante</span></label>
            <input id="Cuadrante" />
        </div>
        <div class="col-xs-12 col-sm-6 col-md-2 col-lg-2 altoControl ">
            <label id=""><span id="EmbarqueMarcadoImpreso">Encintado</span></label>
            <input type="radio" class="radioBtnImpreso radio-responsive" name="Impreso" style="width:auto;" value="1" /><span id="EmbarqueMarcadoRadioSi">Sí</span>
            <input type="radio" class="radioBtnImpreso radio-responsive" name="Impreso" style="width:auto;" value="0" checked="checked"/><span id="EmbarqueMarcadoRadioNo">Todos</span>
        </div>

        <div class="col-sm-6 col-md-2 col-lg-2">
            <label id="">&nbsp;</label>
            <button type="button" id="btnAgregar" class="btn btn-blue"><span id="EmbarqueMarcadoBtnVer">Ver</span></button>

        </div>
    </div>
    @*GRID*@
    <div class="row addedSectionInLine">
        <div id="ContenedorGrid" class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div id="grid" data-role="grid" class="k-grid k-widget">
            </div>
        </div>
        <input id="hdAccionFiltrado" type="hidden" />
    </div>
    @*Menu Inferior*@
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="GuardarPie" onclick="javascript:void(0);" type="button" class="btn btn-yellow"><span id="botonGuardar3"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar1"><span id="botonGuardar4"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevo1"><span id="botonGuardarYNuevo1"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar1"></span></a>
                </div>
                <div id="EntregaresultadosDivINF" class="button-section">
                    <a id="EntregaresultadosINF" class="btn btn-primary"><span id="">Carga de planas</span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones2"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones">
                            
                            <li><a id="ImprimirTravel2" href="#"><span id="EmbarqueMarcadoImprimirTravelMasivo2">Imprimir Traveler Masivo</span></a></li>
                        </ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript" src="~/Scripts/AppComponents/Embarque/Encintado/Ajax.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Embarque/Encintado/Handlers.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Embarque/Encintado/Render.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Embarque/Encintado/Captura.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Embarque/Encintado/Seguridad.js"></script>


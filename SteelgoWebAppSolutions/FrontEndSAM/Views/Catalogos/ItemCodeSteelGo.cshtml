                        $CatalogoICSteelgo.CatalogoICSteelgo.read({}, { data: JSON.stringify(data), token: Cookies.get("token") }).done(function (data) {
                            if (Error(data)) {
                                if (data === null) {
                                    e.model.CedulaA = "";
                                    e.model.CedulaB = "";
                                    e.model.CedulaC = "";
                                    e.model.Libra = "";
                                    
                                    e.model.Inch = "";
                                    e.model.MM = "";

                                    $("#grid").data("kendoGrid").refresh();

                                    e.model.TieneD2 = disabledDiametro2 ? "No" : "Si";
                                    $("#grid").data("kendoGrid").editRow($("#grid tr[data-uid=" + e.container[0].getAttribute("data-uid") + "]"));
                                    $("[name='Inch']").data("kendoComboBox").enable(true);
                                    $("[name='MM']").data("kendoComboBox").enable(true);

                                    displayMessage("ItemCodeSteelGo0021", "", '2');
                                    //$("[name='" + lastEditedDocument + "']").focus();
                                    e.preventDefault();

									//e.stopPropagation();
									return false;
                                }
                                else {
                                    if (data.Correcta == false)
                                    {
                                            //Si existe el diametro seleccionado el combo de espesor se llena con los espesores segun el catalogo de cedulas
                                            $Diametros.Diametros.read({}, { diametro: e.model.Diametro1, token: Cookies.get("token") }).done(function (result) {
                                                existeDiametro = result;
                                                if (existeDiametro) {
                                                    //Obtengo los datos de Espesor en MM
                                                    $ValidarCedulas.ValidarCedulas.read({}, { diametro1ID: e.model.Diametro1ID, espesorIN: false, token: Cookies.get("token") }).done(function (result) {
                                                        dsEspesorMM = result;
                                                        $("[name='MM']").data("kendoComboBox").enable(true);
                                                        $("[name='MM']").data("kendoComboBox").dataSource.data(dsEspesorMM);
                                                    });

                                                    ////Obtengo los datos de espesor en IN
                                                    $ValidarCedulas.ValidarCedulas.read({}, { diametro1ID: e.model.Diametro1ID, espesorIN: true, token: Cookies.get("token") }).done(function (result) {
                                                        dsEspesorIN = result;
                                                        $("[name='Inch']").data("kendoComboBox").enable(true);
                                                        $("[name='Inch']").data("kendoComboBox").dataSource.data(dsEspesorIN);

                                                    });
                                                }
                                                else {
                                                    //Obtengo los datos de Espesor en MM
                                                    $Espesor.Espesor.read({}, { token: Cookies.get("token") }).done(function (result) {
                                                        dsEspesorMM = result;
                                                        $("[name='MM']").data("kendoComboBox").enable(true);
                                                        $("[name='MM']").data("kendoComboBox").dataSource.data(dsEspesorMM);
                                                    });

                                                    //Obtengo los datos de espesor en IN
                                                    $Espesor.Espesor.read({}, { inches: true, token: Cookies.get("token") }).done(function (result) {
                                                        dsEspesorIN = result;
                                                        $("[name='Inch']").data("kendoComboBox").value(EspesorINvalue);
                                                        $("[name='Inch']").data("kendoComboBox").enable(true);
                                                    });
                                                }
                                            });
                                    }
                                    else
                                    {
                                        //data = data[0];
                                        e.model.CedulaID = data.CedulaID;
                                        e.model.CedulaA = data.CedulaA;
                                        e.model.CedulaB = data.CedulaB;
                                        e.model.Libra = data.CedulaC;
                                        e.model.Inch = data.CedulaIn;
                                        e.model.MM = data.CedulaMM;
                                        //e.model.Espesor = data.Espesor;
                                        //e.model.Peso=data.Peso;
                                        //e.model.Area=data.Area;
                                        $("#grid").data("kendoGrid").refresh();
                                        e.model.TieneD2 = disabledDiametro2 ? "No" : "Si";
                                        //if (disabledDiametro2) {
                                        //    obtenerObjetoD2.enable(false);
                                        //};
                                        $("#grid").data("kendoGrid").editRow($("#grid tr[data-uid=" + e.container[0].getAttribute("data-uid") + "]"));
                                        $("[name='Inch']").data("kendoComboBox").enable(false);
                                        $("[name='MM']").data("kendoComboBox").enable(false);
                                        $("[data-container-for='Peso'] input:first").focus();
                                    }
                                }
                            }//ends Error(data)
                            else {
                                e.model.CedulaA = "";
                                e.model.CedulaB = "";
                                e.model.CedulaC = "";
                                e.model.Libra = "";
                                e.model.Inch = "";
                                e.model.MM = "";
                                e.model.Peso = "";
                                e.model.Area = "";
                                $("#grid").data("kendoGrid").refresh();
                                e.model.TieneD2 = disabledDiametro2 ? "No" : "Si";
                                //if (disabledDiametro2) {
                                //    obtenerObjetoD2.enable(false);
                                //};

                                $("#grid").data("kendoGrid").editRow($("#grid tr[data-uid=" + e.container[0].getAttribute("data-uid") + "]"));

                                displayMessage("notificationslabel0008", data.ReturnMessage, '2');
                                //$("[name='" + lastEditedDocument + "']").focus();
								e.preventDefault();
								//e.stopPropagation();
                                return false;
                            }
                        });
                    } else {
                        e.model.CedulaA = "";
                        e.model.CedulaB = "";
                        e.model.CedulaC = "";
                        e.model.Libra = "";
                        $("#grid").data("kendoGrid").refresh();
                        e.model.TieneD2 = disabledDiametro2 ? "No" : "Si";
                        //if (disabledDiametro2) {
                        //    obtenerObjetoD2.enable(false);
                        //};
                        $("#grid").data("kendoGrid").editRow($("#grid tr[data-uid=" + e.container[0].getAttribute("data-uid") + "]"));
                        displayMessage("ItemCodeSteelGo0022", "", '1');
                        //$("[name='Diametro1']").data("kendoComboBox").focus();
						e.preventDefault();
						//e.stopPropagation();
                        return false;
                    }
                });
            },
            update: function (e) {
            },
            cancel: function (e) {
                editado = false;
            },
            remove: function (e) {
                if (e.model.id !== null && e.model.id !== "") {
                    $("#grid").data("kendoGrid").options.dataSource.transport.destroy.url = $CatalogoICSteelgo.CatalogoICSteelgo.urlNoId + "?token=" + Cookies.get("token") + "&id=" + e.model.id + "&diametrosICS=" + e.model.Rel_ICS_DiametroID;
                } else {
                    $("#grid").data("kendoGrid").options.dataSource.transport.destroy.url = "delete";
                    displayMessage("ItemCodeSteelGo0023", "", '2');
                }
            },
            dataBound: function (e) {
                quickHeadFilter($("#grid").data("kendoGrid"));
            },
        });
        applySecurityPolicy(false);
    };

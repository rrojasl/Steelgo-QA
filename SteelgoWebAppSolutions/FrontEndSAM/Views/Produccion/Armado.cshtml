@{
    ViewBag.Title = "Armado";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section breadcrumb {
    <li>
        <a href="@Url.Action("landing", "Home")"><span id="lblInicio"></span></a>
    </li>
    <li>
        <a href="#"><span id="CapturaArmadoBreadcrumbProduccion"></span></a>
    </li>
    <li class="active">
        <a href="@Url.Action("Armado", "Produccion")"><span id="CapturaArmadoBreadcrumb"></span></a>
    </li>
}
<div id="FormCapturaArmado" class="form clearfix col-xs-12 col-sm-12 col-md-12 col-lg-12">
    @*Menu superior*@
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="Guardar" onclick="javascript:void(0);" type="button" class="btn btn-yellow accionGuardar"><span id="botonGuardar"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar" class="accionGuardar"><span id="botonGuardar2"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevo" class="accionGuardarYNuevo"><span id="botonGuardarYNuevo"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar"></span></a>
                </div>
                <div id="ArmadoSoldaduraDivSuperior" class="button-section">
                    <a id="ArmadoSoldaduraSuperior" href="@Url.Action("Soldadura","Fabricacion")" class="btn btn-primary"><span id="armadoSoldaduraSup"></span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones1"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones"></ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
    @*Controles Superior*@
    <div class="row addedSectionInLineWithoutColor" id="FieldSetView">
        <div class="col-xs-12 col-sm-4 col-md-2 col-lg-2 altoControl">
            <div class="btn-group " data-toggle="buttons" role="group">
                <label>&nbsp;</label>
                <div class="btn btn-primary" id="styleReporte">
                    <input type="radio" name="TipoAgregado" style=" width:auto;" value="Reporte" class="radioTipoAgregado" id="TipoAgregadoReporte" /><span id="lblSpool"></span>
                </div>
                <div class="btn btn-primary" id="styleListado">
                    <input type="radio" name="TipoAgregado" style=" width:auto;" value="Listado" class="radioTipoAgregado" id="TipoAgregadoListado" /><span id="lblJunta"> </span>
                </div>
            </div>
        </div>
        <div class="col-xs-6 col-sm-6 col-md-4 col-lg-2 altoControl" id="CapturaArmadoHeaderSpoolIDDiv">
            <label id="lblSpoolID"></label>
            <input style="width:40%" id="InputOrdenTrabajo" class="item-select general-input" />-<input id="InputID" placeholder="ID" style="width:40%;" />
        </div>
        <div id="JuntaDiv">
            <div class="col-xs-4 col-sm-2 col-md-2 col-lg-1 altoControl">
                <label id="lblJunta1"></label>
                <input id="Junta" />
            </div>
        </div>
        <div class="col-xs-12 col-sm-8 col-md-4 col-lg-3 altoControl" id="MuestraDiv">
            <label id="lblMostrar"></label>
            <input type="radio" name="Muestra" class="radio-responsive" style=" width:auto;" value="Sin Capturar" />&nbsp;<span id="lblSinCaptura"></span>&nbsp;&nbsp;
            <input type="radio" name="Muestra" class="radio-responsive" style=" width:auto;" value="Todos" />&nbsp;<span id="lblTodos"> </span>

        </div>
        <div class="col-xs-12 col-sm-2 col-md-12 col-lg-1 altoControl">
            <label id="">&nbsp;</label>
            <button type="button" id="ButtonAgregar" class="btn btn-blue botonSizeGeneral"><span id="botonAgregar"></span></button>

        </div>
        <div class="col-sm-6 col-md-6 col-lg-6">
            <label id="LabelProyecto" style="display:none;"> </label>
        </div>
    </div>
    @*GRID*@
    <div class="row addedSectionInLine">
        <div class="col-xs-12 col-sm-4 col-md-2 col-lg-2 altoControl" id="FechaArmadoDiv">
            <label id="lblFecha"></label>
            <input id="FechaArmado" />
        </div>
        <div class="col-xs-12 col-sm-4 col-md-3 col-lg-3 altoControl" id="inputTuberoDiv">
            <label id="lblTubero"></label>
            <input id="inputTubero" class="" />
        </div>
        <div class="col-xs-12 col-sm-4 col-md-3 col-lg-3 altoControl" id="inputTallerDiv">
            <label id="lblTaller"></label>
            <input id="inputTaller" class="" />
        </div>
        <div class="col-xs-12 col-sm-8 col-md-4 col-lg-2 altoControl" id="divLlenadoMasivo">
            <label id="lblPlanchar"></label>
            <input type="radio" name="LLena" class="radio-responsive" style="width:auto;" value="Todos" />&nbsp;<span id="lblTodos1"></span>&nbsp;&nbsp;
            <input type="radio" name="LLena" class="radio-responsive" style="width:auto;" value="Vacios" />&nbsp;<span id="lblVacios"></span>
        </div>
        <div class="col-xs-12 col-sm-2 col-md-12 col-lg-2">
            <label id="">&nbsp;</label>
            <button type="button" id="ButtonPlanchar" class="btn btn-blue botonSizeGeneral"><span id="botonPlanchar"></span></button>
        </div>
        <div id="ContenedorGrid" class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div id="grid" data-role="grid" class="k-grid k-widget">
            </div>
        </div>
        <input id="hdAccionFiltrado" type="hidden" />
    </div>

    @*GRID POPUP*@
    <div id="windowGrid" style="display: none; z-index:inherit;">

        <div class="row col-sm-12 col-md-12 col-lg-12" style="width: 100%; padding-right:0px; ">
            <div class="form-group">
                <div id="ContenedorGridPopUp">
                    <div id="gridPopUp" data-role="grid" class="k-grid k-widget">
                    </div>
                </div>
            </div>
        </div>
        <div class="row col-sm-12 col-md-12 col-lg-12" style="padding-right:0px;">
            <div class="col-sm-2 col-md-2 col-lg-2">

            </div>
            <div class="col-sm-3 col-d-3 col-lg-3">
                <label id="">&nbsp;</label>
                <a class=" btn btn-primary " href="#" id="GuardarTrabajosAdicionales">
                    <span id="botonAgregar2"></span>
                </a>
            </div>
            <div class="col-sm-3 col-d-3 col-lg-3">
                <label id="">&nbsp;</label>
                <a class=" btn btn-primary " href="#" id="CancelarTrabajosAdicionales">
                    <span id="botonCancelar2"></span>
                </a>
            </div>
        </div>

    </div>
</div>
@*Menu Inferior*@
<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <div class="formNav clearfix">
            <div class="button-section editButtonSection ">
                <div class="btn-group save-group ">
                    <button id="GuardarPie" onclick="javascript:void(0);" type="button" class="btn btn-yellow accionGuardar"><span id="botonGuardar1"></span></button>

                    <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <span class="caretWhite"></span>
                    </button>
                    <ul class="dropdown-menu">
                        <li><a href="#" id="btnGuardarPie" class="accionGuardar"><span id="botonGuardar3"></span></a></li>
                        <li><a href="#" id="btnGuardarYNuevoPie" class="accionGuardarYNuevo"><span id="botonGuardarYNuevo1"></span></a></li>
                    </ul>
                </div>
                <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar1"></span></a>

            </div>
            <div id="ArmadoSoldaduraDivInferior" class="button-section">
                <a id="ArmadoSoldaduraInferior" href="@Url.Action("Soldadura","Fabricacion")" class="btn btn-primary"><span id="armadoSoldaduraInf"></span></a>
            </div>
            <div class="button-section actionButtonSection">
                <div class="dropdown action-group">
                    <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                        <span id="botonAcciones2"></span>
                        <span class="caretBlue"></span>
                    </a>
                    <ul class="dropdown-menu" aria-labelledby="Acciones"></ul>
                </div>
            </div>
            <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
        </div>
    </div>
</div>
</div>

<script type="text/javascript" src="~/Scripts/AppComponents/Fabricacion/Armado/Ajax.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Fabricacion/Armado/Handlers.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Fabricacion/Armado/Render.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Fabricacion/Armado/Captura.js"> </script>
<script type="text/javascript" src="~/Scripts/AppComponents/Fabricacion/Armado/Seguridad.js"></script>

<script>
        @section JavascriptDocumentReadyFunctions {
        $authorizationModel["Armado"] = $ArmadoModel;
        }
</script>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<style>
    .k-window div.k-window-content {
        overflow-x: scroll;
    }

    .box {
        -moz-border-radius: 2px;
        -webkit-border-radius: 2px;
        background-color: #EAEAEA;
        background: -moz-linear-gradient(top, #FFF, #FFF);
        background: -webkit-gradient(linear, left top, left bottom, color-stop(0.0, #FFF), color-stop(1.0, #FFF));
        border: 1px solid #a09b9b;
        color: #444;
        font-size: 15px;
        padding: 5px 10px;
        padding-top: 5px;
        width: 200px;
    }
     .box:focus {
        background: #FFF;
        -webkit-box-shadow: 0 0 18px #CCC;
        -moz-box-shadow: 0 0 18px #ccc;
        box-shadow: 0 0 18px #CCC;
        -webkit-transform: scale(1);
        -moz-transform: scale(1);
        transform: scale(1);
    }
</style>

@section breadcrumb {
    <li>
        <a href="@Url.Action(" landing", "Home" )"><span id="lblInicio"></span></a>
    </li>
    <li class="active">
        <a href="@Url.Action("CobroOC", "Dynasol")"><span id="titleCobroOC"></span></a>
    </li>
}
<div id="FormCapturaReportePruebas" class="form clearfix col-xs-12 col-sm-12 col-md-12 col-lg-12">
    @*Menu superior*@
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="btnGuardarSup1" onclick="javascript:void(0);" type="button" class="btn btn-yellow"><span id="botonGuardar"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardarSup2"><span id="botonGuardar2"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevoSup"><span id="botonGuardarYNuevo"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar"></span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones1"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones">
                            <li><a href="#" id="btnCargaCsv"><span id="botonCargaExcel"></span></a></li>
                            <li><a href="#" id="btnDescargaCsv"><span id="botonDescargaExcel"></span></a></li>
                        </ul>                        
                    </div>
                </div>
                <input type="file" name="File Upload" id="files" class="hidden" accept=".csv" />
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
    @*Controles Superior*@


    @*Controles Superior*@
    <div id="FieldSetView" class="row addedSectionInLineWithoutColor">
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-2 altoControl" id="divProyecto">
            <label id="DynasolOrdenCompra" class="security_required"></label>
            <input id="inputOrdenCompra" class="" />
        </div>
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-2 altoControl" id="divProyecto">
            <label id="lblCliente"></label>
            <input id="inputCliente" class="" />
        </div>
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-1 altoControl" id="divProyecto">
            <label id="lblMoneda"></label>
            <input id="inputMoneda" class="" />
        </div>
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-1 altoControl" id="divProyecto">
            <label id="lblCerrar"></label>
            <input type="checkbox" id="CheckCerrar" class="" />
        </div>
        <div class="col-xs-12 col-sm-6 col-md-2 col-lg-2 altoControl">
            <label id="lblMostrar"></label>
            <input type="radio" name="Muestra" class="radio-responsive" style="width:auto;" value="SinCaptura" /><span id="lblSinCaptura"></span>
            <input type="radio" name="Muestra" class="radio-responsive" style="width:auto;" value="Todos" /><span id="lblTodos"> </span>
        </div>
        <div class="col-sm-1 col-md-1 col-lg-1">
            <label id="">&nbsp;</label>
            <button type="button" id="btnMostrar" class="btn btn-blue botonSizeGeneral"><span id="botonMostrar"></span></button>
        </div>
    </div>

    @*GRID POPUP*@
    <div id="windowGrid" style="display: none; z-index:inherit;">
        <div class="row col-sm-12 col-md-12 col-lg-12 col-xs-12 text-center" >
            <div class="row">
                <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 altoControl" >
                    <label id="lblCliente1" style="padding-bottom: 5px;" class="security_required"></label>                    
                    <input id="inputNombreCliente" type="text" class="box" />
                     @*<input id="inputDibujo" class="item-select general-input" />*@
                </div>
                <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 altoControl" >
                    <label id="lblDireccion" style="padding-bottom: 5px;"></label>
                    <input id="inputDireccion" type="text" class="box" />                    
                </div>                
            </div>
            <div class="row">
                <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 altoControl">
                    <label id="lblCiudad" style="padding-bottom: 5px;"></label>
                    <input id="inputCiudad" type="text" class="box" />                    
                </div>
                <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 altoControl">
                    <label id="lblEstado" style="padding-bottom: 5px;"></label>
                    <input id="inputEstado" type="text" class="box" />                    
                </div>
                <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 altoControl">
                    <label id="lblPais" style="padding-bottom: 5px;"></label>
                    <input id="inputPais" type="text" class="box" />                    
                </div>
            </div>
            <div class="row">
                <div>
                    <a class="btn btn-primary" href="#" id="GuardarCliente"><span id="lblGuardar"></span></a>
                    &nbsp;
                    <a class="btn btn-primary" href="#" id="btnCerrarPopup"><span id="botonCerrar"></span></a>                
                </div>
                
            </div>                        
        </div>    
    </div>


    @*GRID PARA RT*@
    <div class="row addedSectionInLine" id="gridRT">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="row" id="FieldSetView3">
                <div id="ContenedorGrid" class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <div id="grid" data-role="grid" class="k-grid k-widget">
                    </div>
                </div>
            </div>
        </div>
    </div>

    @*Menu Inferior*@
    <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="btnGuardarInf1" onclick="javascript:void(0);" type="button" class="btn btn-yellow"><span id="botonGuardar3"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardarInf2"><span id="botonGuardar4"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevoInf"><span id="botonGuardarYNuevo1"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar1"></span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones2"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones">
                            <li><a href="#" id="btnCargaCsv1"><span id="botonCargaExcel1"></span></a></li>
                            <li><a href="#" id="btnDescargaCsv1"><span id="botonDescargaExcel1"></span></a></li>
                        </ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript" src="~/Scripts/AppComponents/OrdenCompraDynasol/CobroOC/Ajax.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/OrdenCompraDynasol/CobroOC/Handlers.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/OrdenCompraDynasol/CobroOC/Render.js"></script>

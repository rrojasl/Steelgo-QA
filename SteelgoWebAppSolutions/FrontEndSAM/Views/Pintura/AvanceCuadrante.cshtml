@{
    ViewBag.Title = "Avance de Cuadrante";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section breadcrumb {
    <li>
        <a href="@Url.Action("landing", "Home")"><span id="lblInicio"></span></a>
    </li>
    <li>
        <a href="#"><span id="PinturaBreadcrumb"></span></a>
    </li>
    <li class="active">
        <a href="@Url.Action("AvanceCuadrante", "Pintura")"><span id="PinturaAvanceIntermedioAcabadoBreadcrumb"></span></a>
    </li>
}

<div id="FormCapturaAvanceIntAcabado" class="form clearfix col-xs-12 col-sm-12 col-md-12 col-lg-12">
    @*Menu superior*@
    <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="row">
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <div class="button-section editButtonSection">
                            <div class="btn-group save-group">
                                <button id="Guardar" onclick="javascript:void(0);" type="button" class="btn btn-yellow accionGuardar"><span id="botonGuardar2"></span></button>
                                <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle botonDeplegaMenu" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <span class="caretWhite"></span>
                                </button>
                                <ul class="dropdown-menu">
                                    <li><a href="#" id="btnGuardar" class="accionGuardar"><span id="botonGuardar"></span></a></li>
                                    <li><a href="#" id="btnGuardarYNuevo"><span id="botonGuardarYNuevo"></span></a></li>
                                </ul>
                            </div>

                            <a id="btnCancelar" class="btn btn-black btnCancelar"><span id="botonCancelar"></span></a>
                        </div>
                        <div id="CapturaAvanceIntAcabadoDivSuperior" class="button-section">
                            <a id="CapturaAvanceIntAcabadoSuperior" href="@Url.Action("LotesCapturaPrueba", "Pintura")" class="btn btn-primary"><span id="lblLotesCapturaReporteSup"></span></a>
                        </div>
                        <div class="button-section actionButtonSection">
                            <div class="dropdown action-group">
                                <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                                    <span id="botonAcciones1"></span>
                                    <span class="caretBlue"></span>
                                </a>
                                <ul class="dropdown-menu" aria-labelledby="Acciones">
                                    <li><a href="#" id=""><span id="">Captura de pruebas por Spool</span></a></li>
                                </ul>
                            </div>
                        </div>
                        <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @*Controles Superiores*@
    <div class="row  addedSectionInLineWithoutColor" id="ProcesoPinturaDiv">
        <div class="row col-xs-12 col-sm-4 col-md-2 col-lg-6 altoControl" id="carroDiv">
            <label><span id="lblProcesosPintura"></span></label>
            <div class="btn-group " data-toggle="buttons" role="group">
                <label>&nbsp;</label>
                <div class="btn btn-primary" id="stylePPShotBlast">
                    <input type="radio" name="ProcesoPintura" style=" width:auto;" value="1" class="radioProcesoPintura" /><span id="lblSPShotBlast"></span>
                </div>
                <div class="btn btn-primary" id="stylePPPrimario">
                    <input type="radio" name="ProcesoPintura" style=" width:auto;" value="2" class="radioProcesoPintura" /><span id="lblSPPrimario"> </span>
                </div>
                <div class="btn btn-primary" id="stylePPIntermedio">
                    <input type="radio" name="ProcesoPintura" style=" width:auto;" value="3" class="radioProcesoPintura" /><span id="lblSPIntermedio"> </span>
                </div>
                <div class="btn btn-primary" id="stylePPAvanzado">
                    <input type="radio" name="ProcesoPintura" style=" width:auto;" value="4" class="radioProcesoPintura" /><span id="lblSPAvanzado"> </span>
                </div>
            </div>
        </div>
    </div>
    <div class="row " id="FieldSetView">
        <div class="col-sm-12 col-md-12 col-lg-12">

            <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2 " id="diTipoPrueba">
                <label id="lblProyecto"></label>
                <input id="inputProyecto" />
            </div>
            <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2 " id="diTipoPrueba">
                <label id="lblZona">Zona</label>
                <input id="inputZona" />
            </div>
            <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2 " id="InputCuadranteDiv">
                <label id="lblCuadrante"></label>
                <input id="inputCuadrante" />
            </div>
            <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2 " id="divPinturaComponenteComposicion">
                <label id="lblSistemaPintura"></label>
                <input id="inputSistemaPintura" />
            </div>
            <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2 " id="divColor">
                <label id="lblColor"></label>
                <input id="inputColor" />
            </div>
            <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2 altoControl">
                <label id="lblMostrar"></label>
                <input type="radio" name="Muestra" class="radio-responsive" style="width:auto;" value="SinCaptura" /><span id="lblSinCaptura"></span>
                <input type="radio" name="Muestra" class="radio-responsive" style="width:auto;" value="Todos" /><span id="lblTodos"> </span>
            </div>
            <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2 altoControl FieldSetView">
                <label></label>
                <button class="btn btn-blue" id="btnMostrar"><span>Mostrar</span></button>
            </div>
        </div>

    </div>
    
    <div class="row ">
        <br />
        <div class="col-xs-6 col-sm-6 col-md-4 col-lg-2 altoControl" id="CapturaArmadoHeaderSpoolIDDiv">
            <label id="lblSpoolID"></label>
            <input style="width:40%" id="InputOrdenTrabajo" class="item-select general-input" />-<input id="InputID" placeholder="ID" style="width:40%;" />
        </div>
        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2 FieldSetView">
            <label id="">&nbsp;</label>
            <button class="btn btn-blue" id="btnAgregar"><span>Agregar</span></button>
        </div>
    </div>
    @*GRID*@
    <div class="row addedSectionInLine ">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="row" id="FieldSetView2">
                <div class="col-xs-12 col-sm-6 col-md-2 col-lg-2 altoControl" id="divFecha">
                    <label>Fecha Proceso</label>
                    <input id="inputFechaProceso" />
                </div>
                <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2 altoControl altoControl" id="divPintor">
                    <label id="lblPintor"></label>
                    <input id="inputPintor" />
                </div>
                <div class="col-xs-12 col-sm-6 col-md-5" id="divAgregarComponentesReductoresDinamicos" style="width:auto; max-width:50%">
                </div>
                <div class="btn-group col-xs-12 col-sm-6 col-md-2 col-lg-2" role="group" id="LlenaCapturaAvanceIntAcabadoDiv">
                    <label id="lblPlanchar"></label>
                    <input type="radio" id="LlenaTodos" name="LLena" style="width:auto;" value="Todos" /><span id="lblTodos1"></span>
                    <input type="radio" id="LlenaVacios" name="LLena" style="width:auto;" value="Vacios" /><span id="lblVacios"></span>

                </div>
                <div class="col-xs-2 col-sm-2 col-md-2 col-lg-1">
                    <label id="">&nbsp;</label>
                    <button type="button" id="ButtonPlanchar" class="btn btn-blue"><span id="botonPlanchar"></span></button>
                </div>
            </div>
            <div class="row">
                <div id="ContenedorGrid" class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <div id="grid" data-role="grid" class="k-grid k-widget"></div>
                </div>
            </div>
        </div>
        <input id="hdAccionFiltrado" type="hidden" />
    </div>

    <div id="window"></div>

    @*Menu Inferior*@
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="GuardarPie" onclick="javascript:void(0);" type="button" class="btn btn-yellow accionGuardar"><span id="botonGuardar3"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle botonDeplegaMenu" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar4" class="accionGuardar"><span id="botonGuardar4"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevo2"><span id="botonGuardarYNuevo1"></span></a></li>
                        </ul>
                    </div>

                    <a id="btnCancelar" class="btn btn-black btnCancelar"><span id="botonCancelar1"></span></a>
                </div>
                <div id="CapturaAvanceIntAcabadoDivInferior" class="button-section">
                    <a id="CapturaAvanceIntAcabadoInferior" href="@Url.Action("LotesCapturaPrueba", "Pintura")" class="btn btn-primary"><span id="lblLotesCapturaReporteInf"></span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones2"></span>
                            <span class="caretBlue" id="lblAccion002"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones">
                            <li><a href="#" id=""><span id="">Captura de pruebas por Spool</span></a></li>
                        </ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
</div>
<div id="ventanaConfirmCapturaProcesoPintura"></div>
<div id="ventanaConfirmCapturaLineaCaptura"></div>

<script type="text/javascript" src="~/Scripts/AppComponents/Pintura/AvanceCuadrante/Render.js"></script>

@{
    ViewBag.Title = "Lote Captura";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section breadcrumb {
    <li>
        <a href="@Url.Action("landing", "Home")"><span id="lblInicio"></span></a>
    </li>
    <li>
        <a href="#"><span id="PinturaBreadcrumb"></span></a>
    </li>
    <li class="active">
        <a href="@Url.Action("LotesCapturaReporte", "Pintura")"><span id="PinturaLotesCapturaPruebaBreadcrumb"></span></a>
    </li>
}

<div id="FormListadoRequisicion" class="form clearfix col-xs-12 col-sm-12 col-md-12 col-lg-12">
    @*Menu Superior*@
    <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="row">
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <div class="button-section editButtonSection">
                            <div class="btn-group save-group">
                                <button id="Guardar" onclick="javascript:void(0);" type="button" class="btn btn-yellow"><span id="botonGuardar2"></span></button>
                                <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <span class="caretWhite"></span>
                                </button>
                                <ul class="dropdown-menu">
                                    <li><a href="#" id="btnGuardar"><span id="botonGuardar"></span></a></li>
                                    <li><a href="#" id="btnGuardarYNuevo"><span id="botonGuardarYNuevo"></span></a></li>
                                </ul>
                            </div>
                            <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar"></span></a>
                        </div>
                        @*<div id="LotesCapturaReporteDivSuperior" class="button-section">
                            <a id="LotesCapturaReporteSuperior" href="@Url.Action("Consulta", "Embarque")" class="btn btn-primary"><span id="lblDescargaDeCarroSup"></span></a>
                        </div>*@
                        <div class="button-section actionButtonSection">
                            <div class="dropdown action-group">
                                <a id="Acciones" data-target="#" href="#/" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                                    <span id="botonAcciones1"></span>
                                    <span class="caretBlue"></span>
                                </a>
                                <ul class="dropdown-menu" aria-labelledby="Acciones">
                                    <li><a href="#/" id="btnGenerarReporte"><span id="botonGenerarReporte"></span></a></li>
                                </ul>
                            </div>
                        </div>
                        <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @* Inicia Cuerpo de Pagina*@
    <div class="row" id="FieldSetView">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="row">
                <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2 altoControl" id="CapturaArmadoHeaderSpoolIDDiv">
                        <label id="lblProyecto"></label>
                        <input id="inputProyecto" class="" />
                </div>
                <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2 altoControl" id="CapturaArmadoHeaderSpoolIDDiv1">
                    <label>Proceso</label>
                    <input id="inputProceso" class="" />
                </div>
                <div class="col-xs-12 col-sm-4 col-md-2 col-lg-2 altoControl" id="CapturaArmadoHeaderSpoolIDDiv">
                    <label id="lblSistemaPintura"></label>
                    <input id="inputSistemaPintura" class="" />
                </div>
                <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2 altoControl" id="divColor" style="display:none">
                    <label id="lblColor"></label>
                    <input id="inputColor" />
                </div>
                <div class="col-xs-12 col-sm- col-md-2 col-lg-2 altoControl" id="CapturaArmadoHeaderSpoolIDDiv1">
                    <label id="lblPrueba"></label>
                    <input id="inputPrueba" class="" />
                </div>
                <div class="col-xs-12 col-sm- col-md-2 col-lg-2 altoControl" id="CapturaArmadoHeaderSpoolIDDiv1">
                    <label>Fecha Lote</label>
                    <input id="inputFechaLote" class="" />
                </div>
                <div class="col-xs-12 col-sm- col-md-2 col-lg-2 altoControl" id="CapturaArmadoHeaderSpoolIDDiv1">
                    <label id="lblLote"></label>
                    <input id="inputLote" class="" />
                </div>
                <div class="col-xs-12 col-sm-8 col-md-3 col-lg-2 altoControl">
                    <label id="lblMostrar"></label>
                    <input type="radio" name="Muestra" class="radio-responsive" style="width:auto;" value="SinCaptura" /><span id="lblSinCaptura"></span>
                    <input type="radio" name="Muestra" class="radio-responsive" style="width:auto;" value="Todos" /><span id="lblTodos"> </span>
                </div>
                <div class="col-xs-12 col-sm-4 col-md-1 col-lg-2" id="CapturaArmadoHeaderSpoolIDDiv1">
                    @*<label id="">&nbsp;</label>*@
                    <button id="btnBuscar" onclick="javascript:void(0);" type="button" class="btn btn-blue"><span>Mostrar</span></button>
                </div>
                <div class="col-xs-12 col-sm-4 col-md-1 col-lg-2" id="CapturaArmadoHeaderSpoolIDDiv1">
                    <label id=""># Pruebas Req</label>
                    <label id="">2</label>
                </div>
                <div class="col-xs-12 col-sm-4 col-md-1 col-lg-2" id="CapturaArmadoHeaderSpoolIDDiv1">
                    <label id=""># Pruebas Ejec</label>
                    <label id="">0</label>
                </div>
                <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2" id="CapturaArmadoHeaderSpoolIDDiv1">
                    <label id="lblCerrarLote"></label>
                    <input type="checkbox" style="width:auto;" id="chkCerrarLote" />
                </div>
            </div>
        </div>
    </div>
    <div class="row addedSectionInLine ">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                        <div id="ContenedorGrid" class="row">
                            <div id="grid" name="grid-Patio" data-role="grid" class="k-grid k-widget">
                            </div>
                            <input id="hdAccionFiltrado" type="hidden" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    @* Finaliza cuerpo de pagina *@
    <br />
    <div class="row ">
        <div class="col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="row">
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <div class="button-section editButtonSection">
                            <div class="btn-group save-group">
                                <button id="Guardar" onclick="javascript:void(0);" type="button" class="btn btn-yellow"><span id="botonGuardar3"></span></button>
                                <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <span class="caretWhite"></span>
                                </button>
                                <ul class="dropdown-menu">
                                    <li><a href="#" id="btnGuardar1"><span id="botonGuardar4"></span></a></li>
                                    <li><a href="#" id="btnGuardarYNuevo1"><span id="botonGuardarYNuevo1"></span></a></li>
                                </ul>
                            </div>
                            <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar1"></span></a>
                        </div>
                        @*<div id="LotesCapturaReporteDivInferior" class="button-section">
                            <a id="LotesCapturaReporteInferior" href="@Url.Action("Consulta", "Embarque")" class="btn btn-primary"><span id="lblDescargaDeCarroInf"></span></a>
                        </div>*@
                        <div class="button-section actionButtonSection">
                            <div class="dropdown action-group">
                                <a id="Acciones" data-target="#" href="#/" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                                    <span id="botonAcciones2"></span>
                                    <span class="caretBlue"></span>
                                </a>
                                <ul class="dropdown-menu" aria-labelledby="Acciones">
                                    <li><a href="#/" id="btnGenerarReporte1"><span id="botonGenerarReporte1"></span></a></li>
                                </ul>
                            </div>
                        </div>
                        <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @*GRID POPUP*@
    <div id="windowGrid" style="display: none; z-index:inherit;">
        <div class="row col-sm-12 col-md-12 col-lg-12" style="width: 100%; padding-right:20px;">
            <div class="form-group">
                <div id="ContenedorGridPopUp">
                    <div id="gridPopUp" data-role="grid" class="k-grid k-widget">
                    </div>
                    <input type="hidden" id="Intervalo" />
                </div>
            </div>
        </div>
        <div class="row col-sm-12 col-md-12 col-lg-12">
            <a class="btn btn-primary col-sm-2 col-md-2 col-lg-2 col-lg-offset-4" href="#" id="GuardarDetallePopup">
                <span id="botonGuardar1"></span>
            </a>
            <a class="btn btn-primary col-sm-2 col-md-2 col-lg-2" href="#" id="CerrarDetallePopup">
                <span id="botonCerrar"></span>
            </a>
        </div>
    </div>
</div>

<script type="text/javascript" src="~/Scripts/AppComponents/Pintura/CapturaPruebaLotes/Render.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Pintura/CapturaPruebaLotes/Seguridad.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Pintura/CapturaPruebaLotes/Ajax.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Pintura/CapturaPruebaLotes/Handlers.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Pintura/CapturaPruebaLotes/Captura.js"></script>

<script id="cell-template" type="text/x-kendo-template">
    <span class="#= isInArray(data.date, data.dates) ? 'party' : '' #">#= data.value #</span>
</script>
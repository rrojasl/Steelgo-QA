    <div id="FieldSetView" class="row addedSectionInLineWithoutColor">

        <input id="inputSistemaPinturaID" type="hidden" value="" />

        <div class="col-sm-3 col-md-3 col-lg-3">
            <label id="columnNombre"></label>
            <input id="inputNombre" class="general-input item-select" />
        </div>
        <div class="col-sm-3 col-d-3 col-lg-3 Bginput">

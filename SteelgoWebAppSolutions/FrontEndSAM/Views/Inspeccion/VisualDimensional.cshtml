                        </ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
    @*Controles Superior*@
    <div class="row" id="FieldSetView">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <h6> <span id="InpeccionVisualHeaderDimensional"></span></h6>
        </div>
        <div class="col-xs-4 col-sm-4 col-md-3 col-lg-2 altoControl" id="InputIDDiv">

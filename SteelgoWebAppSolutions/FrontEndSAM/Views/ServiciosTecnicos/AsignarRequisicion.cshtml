@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section breadcrumb {
    <li>
        <a href="@Url.Action("landing", "Home")"><span id="lblInicio"></span></a>
    </li>
    <li>
        <a href="#"><span id="ServiciosTecnicosHeader"></span></a>
    </li>
    <li class="active">
        <a href="@Url.Action("AsignarRequisicion", "ServiciosTecnicos")"><span id="AsignarRequisicion"></span></a>
    </li>
}
<div id="FormAsignarRequisicion" class="form clearfix col-xs-12 col-sm-12 col-md-12 col-lg-12">
    @*Menu superior*@
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="Guardar" onclick="javascript:void(0);" type="button" class="btn btn-yellow"><span id="botonGuardar"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar"><span id="botonGuardar2"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevo"><span id="botonGuardarYNuevo"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar"></span></a>
                </div>
                <div id="EntregaresultadosDivSup" class="button-section">
                    <a id="EntregaresultadosSup" href="@Url.Action("EntregaResultados", "ServiciosTecnicos")" class="btn btn-primary"><span id="AsignarRequisicionEntregaResultados"></span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones1"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones"></ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
    @*Controles Superior*@
    <div id="FieldSetView">
        <div class="row">
            <div class="col-sm-3 col-md-3 col-lg-3">
                <label id="CapturaHeaderMostrar"></label>
                <input type="radio" name="Muestra" style=" width:auto;" value="Sin Capturar" /><span id="CapturaSinAsinar"></span>
                <input type="radio" name="Muestra" style=" width:auto;" value="Todos" /><span id="CapturaTodos"> </span>

            </div>
            <div class="col-sm-2 col-md-2 col-lg-2">
                <label id="AsignarRequisicionHeaderPrueba"></label>
                <input id="inputPrueba" class="" />
            </div>
            <div class="col-sm-2 col-md-2 col-lg-2">
                <label id="AsignarRequisicionHeaderProveedor"></label>
                <input id="inputProveedor" class="" />
            </div>
        </div>
    </div>
    @*GRID*@
    <div class="row addedSectionInLine">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div id="ContenedorGrid" class="row">
                    <div id="grid" data-role="grid" class="k-grid k-widget">
                    </div>
                    <input id="hdAccionFiltrado" type="hidden" />
                </div>
            </div>
        </div>
    </div>
    @*GRID*@

    <div class="row ">
        <div class="col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="GuardarPie" onclick="javascript:void(0);" type="button" class="btn btn-yellow"><span id="botonGuardar3"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar1"><span id="botonGuardar4"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevo1"><span id="botonGuardarYNuevo1"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="CapturaSoldaduraCancelar"></span></a>
                </div>
                <div id="EntregaresultadosDivINF" class="button-section">
                    <a id="EntregaresultadosINF" href="#" class="btn btn-primary"><span id="AsignarRequisicionEntregaResultados2"></span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones2"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones">
                            @*<li><a id="RedirectDashboard" ><span id="DetalleAvisoLlegada0065"></span></a></li>
                                <li><a><span id="DetalleAvisoLlegada0066"></span></a></li>
                                <li><a id="CancelarFolio" href="#"><span id="DetalleAvisoLlegada0083"></span></a></li>*@
                        </ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>


            </div>
        </div>
    </div>
</div>


<script type="text/javascript" src="~/Scripts/AppComponents/ServiciosTecnicos/AsignarRequisicion/Render.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/ServiciosTecnicos/AsignarRequisicion/Ajax.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/ServiciosTecnicos/AsignarRequisicion/Handlers.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/ServiciosTecnicos/AsignarRequisicion/Captura.js"> </script>

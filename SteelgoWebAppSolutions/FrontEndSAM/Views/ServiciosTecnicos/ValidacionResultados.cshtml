@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section breadcrumb {
    <li>
        <a href="@Url.Action("landing", "Home")"><span id="lblInicio"></span></a>
    </li>
    <li>
        <a href="#"><span id="ServiciosTecnicosHeader"></span></a>
    </li>
    <li class="active">
        <a href="@Url.Action("ValidacionResultados", "ServiciosTecnicos")"><span id="lblValidacionResultados"></span></a>
    </li>
}
<div id="FormValidacionResultados" class="form clearfix col-xs-12 col-sm-12 col-md-12 col-lg-12">
    @*Menu superior*@
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="Guardar" onclick="javascript:void(0);" type="button" class="btn btn-yellow accionGuardar"><span id="botonGuardar"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar" class="accionGuardar"><span id="botonGuardar2"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevo"><span id="botonGuardarYNuevo"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar"></span></a>
                </div>
                <div id="ValidacionTresultadosImprimirResultadosDivSup" class="button-section">
                    <a id="ValidacionTresultadosImprimirResultadosSup" href="@Url.Action("ImpresionPruebas", "ServiciosTecnicos")" class="btn btn-primary"><span id="ValidacionResultadosBtnImprimirResultados"></span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones1"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones"></ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
    @*Controles Superior*@
    <div class="row" id="FieldSetView">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="col-sm-2 col-md-2 col-lg-2 " id="containerDiv">
                <label id=""><span id="ValidacionResultadosRequisicion"></span></label>
                <input id="Requisicion" class="" />
            </div>
            <div class="col-sm-2 col-md-2 col-lg-2 ">
                <label id="">&nbsp;</label>
                <a id="btnVer" href="#" class="btn btn-blue"><span id="ValidacionResultadosBtnVer"></span></a>
            </div>
        </div>
    </div>
    @*GRID*@
    <div class="row addedSectionInLine">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div id="ContenedorGrid" class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div id="grid" data-role="grid" class="k-grid k-widget">
                </div>
            </div>

        </div>
    </div>
    @*GRID POPUP*@
    <div id="windowGrid" style="display: none; z-index:inherit;">
        <div class="form-group">
            <label class="col-sm-4 col-md-4 col-lg-4 " id="lblSpoolJunta"></label><label class="col-sm-3 col-md-3 col-lg-3" id="lblUbicacion"></label>
            <div class="col-sm-3 col-md-3 col-lg-3">
                <a class=" btn btn-blue " id="GuardarDefectos"><span id="textoGuardar"></span></a>
            </div>
        </div>
        <input class="" type="hidden" id="hdnPruebaElementoResultadoID" />
        <input class=" " type="hidden" id="hdnRequisicionID" />
        <br />
        <div class="form-group">
            <div id="ContenedorGridPopUp">
                <div id="gridPopUp" data-role="grid" class="k-grid k-widget">
                </div>
            </div>
        </div>

    </div>
    @*Menu Inferior*@
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="Guardar1" onclick="javascript:void(0);" type="button" class="btn btn-yellow accionGuardar"><span id="botonGuardar3"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar1" class="accionGuardar"><span id="botonGuardar4"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevo1"><span id="botonGuardarYNuevo1"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar2" class="btn btn-black"><span id="botonCancelar2"></span></a>
                </div>
                <div id="ValidacionTresultadosImprimirResultadosDivInf" class="button-section">
                    <a id="ValidacionTresultadosImprimirResultadosInf" href="@Url.Action("ImpresionPruebas", "ServiciosTecnicos")" class="btn btn-primary"><span id="ValidacionResultadosBtnImprimirResultados2"></span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones2"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones"></ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript" src="~/Scripts/AppComponents/ServiciosTecnicos/ValidacionResultados/Ajax.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/ServiciosTecnicos/ValidacionResultados/Handlers.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/ServiciosTecnicos/ValidacionResultados/Render.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/ServiciosTecnicos/ValidacionResultados/Captura.js"> </script>
<script type="text/javascript" src="~/Scripts/AppComponents/ServiciosTecnicos/ValidacionResultados/Seguridad.js"></script>

<script>
    @section JavascriptDocumentReadyFunctions {
    $authorizationModel["Validacion Resultados"] = $ValidacionResultadosModel;
    }
</script>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section breadcrumb {
    <li>
        <a href="@Url.Action("landing", "Home")"><span id="lblInicio"></span></a>
    </li>
    <li>
        <a href="#"><span id="ServiciosTecnicosBreadcrumb"></span></a>
    </li>
    <li class="active">
        <a href="@Url.Action("AsignacionRequisicion", "PND")"><span id="ServiciosTecnicosAsignarRequisicionBreadcrumb"></span></a>
    </li>
}
<div id="FormAsignarRequisicion" class="form clearfix col-xs-12 col-sm-12 col-md-12 col-lg-12">
    @*Menu superior*@
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="Guardar" onclick="javascript:void(0);" type="button" class="btn btn-yellow"><span id="botonGuardar"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar"><span id="botonGuardar2"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevo"><span id="botonGuardarYNuevo"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar"></span></a>
                </div>
                <div id="EntregaresultadosDivSup" class="button-section">
                    <a id="EntregaresultadosSup" class="btn btn-primary"><span id="ServiciosTecnicosCapturaReporte"></span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones1"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones"></ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
    @*Controles Superior*@
    <div id="FieldSetView" class="row addedSectionInLineWithoutColor">
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 altoControl" id="divProyecto">
            <label id="lblProyecto"></label>
            <input id="inputProyecto" class="" />
        </div>
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-2 altoControl" id="divPrueba">
            <label id="lblPrueba"></label>
            <input id="inputPrueba" class="" />
        </div>
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-2 altoControl" id="divRequisicion">
            <label id="lblRequisicion"></label>
            <input id="inputRequisicion" class="" />
        </div>
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 altoControl">
            <label id="lblMostrar"></label>
            <input type="radio" name="Muestra" style="width:auto;" class="radio-responsive" value="Sin Capturar" />&nbsp;<span id="lblSinCaptura"></span>&nbsp;
            <input type="radio" name="Muestra" style="width:auto;" class="radio-responsive" value="Todos" />&nbsp;<span id="lblTodos"> </span>
        </div>

    </div>
    @*GRID*@
    <div class="row addedSectionInLine">
        
        <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3" style="display:none;">
            <label id="lblProveedor"></label>
            <input id="inputProveedor" class="" />
        </div>
        <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3" id="divEquipo" style="display:none;">
            <label id="lblEquipo"></label>
            <input id="inputEquipo" class="" />
        </div>
        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2" style="display:none;">
            <label id="lblTurno"></label>
            <input id="inputTurno" class="" />
        </div>
        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2" style="display:none;">
            <label id="lblPlanchar"></label>
            <input type="radio" name="Planchar" style="width:auto;" value="Vacios" /><span id="lblVacio"></span>
            <input type="radio" name="Planchar" style="width:auto;" value="Todos" /><span id="lblTodos1"> </span>
        </div>
        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2" style="display:none;">
            <label id="">&nbsp;</label>
            <button type="button" id="ButtonPlanchar" class="btn btn-blue botonSizeGeneral"><span id="botonPlanchar"></span></button>
        </div>
        
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div id="ContenedorGrid">
                <div id="grid" data-role="grid" class="k-grid k-widget">
                </div>
                <input id="hdAccionFiltrado" type="hidden" />
            </div>

        </div>
    </div>
    @*GRID*@

    @*GRID POPUP*@
    <div id="windowGrid" style="display: none; z-index:inherit;">

        <div class="row col-sm-12 col-md-12 col-lg-12" style="width: 100%; padding-right:20px;">
            <div class="form-group">
                <div id="ContenedorGridPopUp">
                    <div id="gridPopUp" data-role="grid" class="k-grid k-widget">
                    </div>
                </div>
            </div>
        </div>
        <div  class="row col-sm-12 col-md-12 col-lg-12" style="width: 100%; padding-right:20px; left:-7%;">
            <div class="col-sm-5 col-md-5 col-lg-5">

            </div>
            <div class="col-sm-1 col-d-1 col-lg-1">
                <label id="">&nbsp;</label>
                <a class="btn btn-primary " href="#" id="SepararRequisicion">
                    <span id="">Separar</span>
                </a>
            </div>
            <div class="col-sm-1 col-d-1 col-lg-1">
                <label id="">&nbsp;</label>
                <a class="btn btn-primary" href="#" id="CerrarDetalleJunta">
                    <span id="botonCerrar"></span>
                </a>
            </div>
        </div>

    </div>

    <div class="row ">
        <div class="col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="GuardarPie" onclick="javascript:void(0);" type="button" class="btn btn-yellow"><span id="botonGuardar3"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar1"><span id="botonGuardar4"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevo1"><span id="botonGuardarYNuevo1"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar1"></span></a>
                </div>
                <div id="EntregaresultadosDivINF" class="button-section">
                    <a id="EntregaresultadosINF" class="btn btn-primary"><span id="ServiciosTecnicosCapturaReporte1"></span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones2"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones">
                            @*<li><a id="RedirectDashboard" ><span id="DetalleAvisoLlegada0065"></span></a></li>
                                <li><a><span id="DetalleAvisoLlegada0066"></span></a></li>
                                <li><a id="CancelarFolio" href="#"><span id="DetalleAvisoLlegada0083"></span></a></li>*@
                        </ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>


            </div>
        </div>
    </div>
</div>


<script type="text/javascript" src="~/Scripts/AppComponents/PND/AsignarRequisicion/Render.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/PND/AsignarRequisicion/Ajax.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/PND/AsignarRequisicion/Handlers.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/PND/AsignarRequisicion/Captura.js"> </script>

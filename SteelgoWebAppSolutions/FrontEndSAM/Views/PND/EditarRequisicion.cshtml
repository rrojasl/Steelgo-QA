@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section breadcrumb {
    <li>
        <a href="@Url.Action("landing","Home" )"><span id="lblInicio"></span></a>
    </li>
    <li>
        <a href="#"><span id="ServiciosTecnicosBreadcrumb"></span></a>
    </li>
    <li class="active">
        <a href="@Url.Action("EditarRequisicion","PND" )"><span id="ServiciosTecnicosEditarRequisicionBreadcrumb"></span></a>
    </li>
}
<div id="FormEditaRequisicion" class="form clearfix col-xs-12 col-sm-12 col-md-12 col-lg-12">
    @*Menu superior*@
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="Guardar" onclick="javascript:void(0);" type="button" class="btn btn-yellow"><span id="botonGuardar"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar"><span id="botonGuardar2"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevo"><span id="botonGuardarYNuevo"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar"></span></a>
                </div>
                <div id="GenerarRequisicionASignarRequisicionDivSup" class="button-section">
                    <a id="GenerarRequisicionASignarRequisicionSup" href="@Url.Action("AsignacionRequisicion", "ServiciosTecnicos")" class="btn btn-primary"><span id="ServiciosTecnicosAsignarRequisicion"></span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones1"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones"></ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
    @*Campos Mandatorios*@
    <div class="row">
        <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3" id="divProyecto">
            <label id="lblProyecto"></label>
            <input id="inputProyecto" />
        </div>
        <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3" id="divPrueba">
            <label id="lblPrueba"></label>
            <input id="inputTipoPrueba" />
        </div>
        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2" id="divRequisicion">
            <label id="lblRequisicion"></label>
            <input id="inputRequisicion" />
        </div>
        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">
            <label id="lblMostrar"></label>
            <input type="radio" name="Muestra" style="width:auto;" value="SinCaptura" /><span id="lblSinCaptura"></span>
            <input type="radio" name="Muestra" style="width:auto;" value="Todos" /><span id="lblTodos"> </span>
        </div>
    </div>
    @*GRID*@
    <div class="row addedSectionInLine">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="row" id="containerDiv" style="display:none">
                <div class="col-sm-2 col-md-2 col-lg-2">
                    <label id="lblSpoolID"></label>
                    <input style="width:40%" id="InputOrdenTrabajo" class="item-select general-input" />-<input id="InputID" placeholder="ID" style="width:40%;" />
                </div>
                <div class="col-sm-2 col-md-2 col-lg-2" id="divJuntaSpool" style="display:none">
                    <label id="lblJunta"></label>
                    <input id="Junta" />
                </div>
                <div class="col-sm-1 col-md-1 col-lg-1">
                    <label id="">&nbsp;</label>
                    <button type="button" id="ButtonAgregar" class="btn btn-blue"><span id="botonAgregar"></span></button>
                </div>
            </div>
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div id="ContenedorGrid" class="row">
                    <div id="grid" data-role="grid" class="k-grid k-widget"></div>
                    <input id="hdAccionFiltrado" type="hidden" />
                </div>
            </div>
        </div>
     </div>
    @*Menu Inferior*@
    <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="Guardar1" onclick="javascript:void(0);" type="button" class="btn btn-yellow"><span id="botonGuardar3"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar1"><span id="botonGuardar4"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevo1"><span id="botonGuardarYNuevo1"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar1"></span></a>
                </div>
                <div id="GenerarRequisicionASignarRequisicionDivInf" class="button-section">
                    <a id="GenerarRequisicionASignarRequisicionInf" href="~/ServiciosTecnicos/AsignarRequisicion" class="btn btn-primary"><span id="ServiciosTecnicosAsignarRequisicion1"></span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones2"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones"></ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript" src="~/Scripts/AppComponents/PND/EditarRequisicion/Ajax.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/PND/EditarRequisicion/Handlers.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/PND/EditarRequisicion/Render.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/PND/EditarRequisicion/Captura.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/PND/EditarRequisicion/Seguridad.js"></script>